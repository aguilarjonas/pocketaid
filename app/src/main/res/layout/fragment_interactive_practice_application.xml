<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jonas.pocketaid.PracticeFragments.InteractivePracticeApplication">

    <!-- TODO: Update blank fragment layout -->

    <com.example.jonas.pocketaid.PracticeFragments.TouchImage
        android:id="@+id/touchImage"
        android:layout_height="250dp"
        android:src="@drawable/samplecut"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_width="500dp" />


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <TextView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/checkmark"
            android:layout_marginBottom="128dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="28dp"
            android:layout_marginStart="28dp"
            android:id="@+id/textView8" />

        <TextView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/checkmark"
            android:layout_alignBaseline="@+id/textView8"
            android:layout_alignBottom="@+id/textView8"
            android:layout_centerHorizontal="true"
            android:id="@+id/textView9" />

        <TextView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/checkmark"
            android:layout_alignBaseline="@+id/textView9"
            android:layout_alignBottom="@+id/textView9"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="16dp"
            android:layout_marginEnd="16dp" />

        <Button
            android:id="@+id/triggerButton"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:background="@drawable/circleshape"
            android:layout_marginTop="79dp"
            android:layout_alignParentTop="true"
            android:layout_alignRight="@+id/textView9"
            android:layout_alignEnd="@+id/textView9" />

    </RelativeLayout>

</RelativeLayout>
